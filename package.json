  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "bootstrap": "^4.4.1",
    "html-minifier": "^4.0.0",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "popper.js": "^1.16.1",
    "slick-carousel": "^1.8.1"
  },
  "keywords": [
    "gnome",
    "contributors",
    "jekyll"
  ],
